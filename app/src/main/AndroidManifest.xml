<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rflora.homeworkplannerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_temp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Base.MainActivityTabs"
            android:label="@string/title_activity_main_tabs"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.rflora.homeworkplannerapp.Assignments.AddAssignmentActivity"
            android:label="Add Assignment"
            android:parentActivityName="com.rflora.homeworkplannerapp.Base.MainActivityTabs"

        />

    </application>

</manifest>